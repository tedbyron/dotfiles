{
  "breadcrumbs.enabled": false,
  "css.lint.boxModel": "error",
  "css.lint.duplicateProperties": "error",
  "css.lint.float": "error",
  "css.lint.important": "warning",
  "css.lint.importStatement": "warning",
  "css.lint.universalSelector": "error",
  "css.lint.unknownVendorSpecificProperties": "warning",
  "css.lint.zeroUnits": "warning",
  "editor.acceptSuggestionOnCommitCharacter": false,
  "editor.acceptSuggestionOnEnter": "off",
  "editor.find.addExtraSpaceOnTop": false,
  "editor.fontFamily": "Iosevka SS07 Extended",
  "editor.fontLigatures": true,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "explorer.incrementalNaming": "smart",
  "editor.minimap.enabled": false,
  "editor.minimap.maxColumn": 80,
  "editor.minimap.renderCharacters": false,
  "editor.minimap.showSlider": "always",
  "editor.renderLineHighlight": "all",
  "editor.roundedSelection": false,
  "editor.rulers": [80, 100],
  "editor.wordWrap": "on",
  "eslint.format.enable": true,
  "eslint.lintTask.enable": true,
  "explorer.autoReveal": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.openEditors.visible": 0,
  "extensions.ignoreRecommendations": true,
  "files.autoSave": "off",
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.enableCommitSigning": true,
  "git.enableSmartCommit": true,
  "html.format.enable": false,
  "javascript.format.enable": false,
  "json.format.enable": false,
  "prettier.arrowParens": "always",
  "prettier.endOfLine": "lf",
  "prettier.printWidth": 100,
  "prettier.trailingComma": "es5",
  "rust-analyzer.checkOnSave.command": "clippy",
  "rust-analyzer.notifications.cargoTomlNotFound": false,
  "scss.lint.boxModel": "error",
  "scss.lint.duplicateProperties": "error",
  "scss.lint.float": "error",
  "scss.lint.important": "warning",
  "scss.lint.importStatement": "warning",
  "scss.lint.universalSelector": "error",
  "scss.lint.unknownVendorSpecificProperties": "warning",
  "scss.lint.zeroUnits": "warning",
  "search.showLineNumbers": true,
  "search.smartCase": true,
  "telemetry.enableTelemetry": false,
  "telemetry.enableCrashReporter": false,
  "terminal.external.linuxExec": "urxvt",
  "terminal.external.osxExec": "iterm.app",
  "terminal.external.windowsExec": "C:\\Windows\\System32\\WindowsPowerShell\\v1.0\\powershell.exe",
  "terminal.integrated.copyOnSelection": true,
  "terminal.integrated.fontSize": 14,
  "todo-tree.filtering.excludeGlobs": [
    "node_modules/",
    ".cache/",
    "target/",
    "public/"
  ],
  "todo-tree.filtering.ignoreGitSubmodules": true,
  "todo-tree.highlights.defaultHighlight": {
    "icon": "issue-opened",
    "type": "text",
    "foreground": "black",
    "background": "#f57f17",
    "opacity": 80,
    "iconColour": "#f57f17"
  },
  "typescript.format.enable": false,
  "typescript.validate.enable": false,
  "window.nativeTabs": true,
  "window.newWindowDimensions": "inherit",
  "window.title": "${activeEditorMedium}",
  "workbench.editor.tabCloseButton": "off",
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.enableExperiments": false,
  "workbench.fontAliasing": "auto",
  "workbench.iconTheme": "vscode-icons",
  "workbench.productIconTheme": "fluent-icons",
  "workbench.sideBar.location": "right",
  "workbench.startupEditor": "none",
  "zenMode.fullScreen": false,
  "zenMode.hideTabs": false,
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[rust]": {
    "editor.tabSize": 4
  },
  "[twig]": {
    "editor.formatOnSave": false,
    "editor.formatOnPaste": false,
    "editor.formatOnType": false
  },
  "vsicons.dontShowNewVersionMessage": true,
  "security.workspace.trust.untrustedFiles": "open",
  "files.trimTrailingWhitespace": true,
  "workbench.editor.untitled.hint": "hidden",
  "files.eol": "\n",
  "settingsSync.ignoredExtensions": [
    "adpyke.vscode-sql-formatter",
    "clarkyu.vscode-sql-beautify",
    "mtxr.sqltools"
  ],
  "editor.codeLensFontFamily": "Iosevka SS07 Extended",
  "workbench.colorTheme": "Dracula",
  "editor.inlayHints.fontFamily": "Iosevka SS07 Extended, Consolas, 'Courier New', monospace",
  "terminal.integrated.fontFamily": "Iosevka SS07 Extended",
  "rust-analyzer.procMacro.enable": true,
  "editor.tabSize": 2,
  "settingsSync.ignoredSettings": [
    "editor.fontSize",
    "editor.codeLensFontFamily",
    "editor.fontFamily",
    "editor.inlayHints.fontFamily",
    "debug.console.fontSize",
    "terminal.integrated.fontSize",
    "terminal.integrated.fontFamily",
    "scm.inputFontSize"
  ],
  "scm.inputFontSize": 14,
  "[javascript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "eslint.probe": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "html",
    "vue",
    "markdown",
    "jsx",
    "js",
    "ts",
    "tsx"
  ],
  "[javascriptreact]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "editor.scrollbar.horizontal": "visible",
  "editor.scrollbar.vertical": "visible",
  "editor.scrollbar.verticalScrollbarSize": 12,
  "markdown.preview.fontSize": 13,
  "debug.console.fontSize": 14,
  "editor.fontSize": 14,
  "security.workspace.trust.banner": "never"
}